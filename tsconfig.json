{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/Menus/index.js", "app/_layout.jsx", "app/Menus/_layout.js", "app/Menus/folder.js", "app/Menus/label.js", "app/Menus/trash.jsx", "app/+html.tsx", "app/+not-found.js", "app/newNote.jsx", "app/editNote.jsx"  ]
}
